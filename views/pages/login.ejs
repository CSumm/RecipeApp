<% include ../partials/header %>
<div class="loginform">
<h1>Login</h1>

<form class="ui form container" action="/login" method="POST">
  <div class="field">
    <label for="username">Username</label>
    <input type="text" id="username" name="username" />
  </div>
  <div class="field">
    <label for="password">Password</label>
    <input type="password" id="password" name="password" />
  </div>
  <input class="large ui positive basic button right floated" type="submit" />
</form>
</div>
